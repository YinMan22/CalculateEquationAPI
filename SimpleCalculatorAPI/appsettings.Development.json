{
  "SeriLog": {
    "Using": [ "SeriLog.Sinks.Console", "SeriLog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "applogs/Log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
